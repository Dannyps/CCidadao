<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
    <parser>
        <target>docs</target>
    </parser>
    <transformer>
        <target>docs</target>
    </transformer>
    <files>
        <directory>src</directory>
    </files>
    <logging>
    <level>debug</level>
    <paths>
        <default>log/{DATE}.log</default>
        <errors>log/{DATE}.errors.log</errors>
    </paths>
</logging>
</phpdoc>

